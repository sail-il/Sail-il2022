<launch>
    <!-- run pointcloud_to_laserscan node-->
        <node pkg="pointcloud_to_laserscan" type="pointcloud_to_laserscan_node" name="pointcloud_to_laserscan">
                <remap from="cloud_in" to="/quanergy/points"/>
                <remap from="scan" to="/laser/scan"/>
                <rosparam>
                    target_fram: quanergy
                    transform_tolerance: 0.01

                    min_height: -1.0
                    max_height: 10.0

                    angle_min: -1.5708
                    angle_max: 1.5708
                    angle_increment: 0.0087
                    scan_time: 0.3
                    range_min: 0.45
                    range_max: 2.0
                    use_inf: true

                    concurrecy_level: 1


                </rosparam>
        </node>
</launch>

